{
  "name": "niche-rfp-tracker",
  "version": "1.0.0",
  "description": "Professional RFP Tracking & Management System for Niche Waterproofing",
  "main": "backend/app.js",
  "scripts": {
    "start": "concurrently \"npm run backend\" \"npm run frontend\"",
    "dev": "concurrently \"npm run backend:dev\" \"npm run frontend:dev\"",
    "backend": "node backend/app.js",
    "backend:dev": "nodemon backend/app.js",
    "frontend": "cd frontend && npm start",
    "frontend:dev": "cd frontend && npm run dev",
    "frontend:build": "cd frontend && npm run build",
    "setup": "node scripts/setup.js",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "build": "npm run frontend:build",
    "deploy": "./scripts/deploy.sh"
  },
  "keywords": [
    "rfp",
    "tracking",
    "waterproofing",
    "construction",
    "bids",
    "proposals"
  ],
  "author": "Joshua - Niche Waterproofing",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "sqlite3": "^5.1.6",
    "sequelize": "^6.35.2",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "winston": "^3.11.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "imap": "^0.8.19",
    "mailparser": "^3.6.5",
    "pdf-parse": "^1.1.1",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "moment": "^2.29.4",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}